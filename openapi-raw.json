                    "uri": {
                        "title": "Uri",
                        "type": "string",
                        "default": ""
                    },
                    "data_author": {
                        "title": "Data Author",
                        "type": "string"
                    },
                    "data_title": {
                        "title": "Data Title",
                        "type": "string"
                    },
                    "data_title_long": {
                        "title": "Data Title Long",
                        "type": "string"
                    },
                    "data_summary": {
                        "title": "Data Summary",
                        "type": "string"
                    },
                    "data_citation": {
                        "title": "Data Citation",
                        "type": "string"
                    },
                    "data_license": {
                        "title": "Data License",
                        "type": "object"
                    },
                    "data_origin_url": {
                        "title": "Data Origin Url",
                        "type": "string"
                    },
                    "data_formats": {
                        "title": "Data Formats",
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
