                "type": "object",
                "properties": {
                    "boundary_name": {
                        "title": "Boundary Name",
                        "type": "string"
                    },
                    "uri": {
                        "title": "Uri",
                        "type": "string"
                    }
                },
                "description": "Summary information about a top-level package (which is formed from a boundary)"
            },
            "Processor": {
                "title": "Processor",
                "required": [
                    "name",
                    "versions"
